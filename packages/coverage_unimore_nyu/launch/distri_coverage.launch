<launch>
    <node name="coverage_robot_0"  pkg="coverage_unimore_nyu" type="coverage_distributed_robotnode" output="screen">
        <rosparam>
            ids: [0,1,2,3,4,5]
            ROBOTS_NUM: 6
            ROBOT_RANGE: 15.0
            ROBOT_ID: 0
            MODEL_NAME: hummingbird0
            AREA_SIZE_x: 40.0
            AREA_SIZE_y: 40.0
            AREA_LEFT: -20.0
            AREA_BOTTOM: -20.0
            CAMERA_FOV: 150
            use_cbf: false
        </rosparam>
        <remap from="/turtlebot0/cmd_vel" to="/hummingbird0/autopilot/velocity_command"/>
        <remap from="/turtlebot0/odom" to="/hummingbird0/ground_truth/odometry"/>
