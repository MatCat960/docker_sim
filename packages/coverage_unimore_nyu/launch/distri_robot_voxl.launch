    <node name="coverage_commander_voxl4" pkg="coverage_unimore_nyu" type="voxl_commander.py" output="screen" ns="voxl4"/-->
    <!--node name="coverage_localization_voxl4" pkg="coverage_unimore_nyu" type="voxl_localize.py" output="screen" ns="voxl4"/-->

