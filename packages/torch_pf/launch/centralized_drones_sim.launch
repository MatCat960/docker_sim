<?xml version="1.0"?>
<launch>
    <arg name="ROBOTS_NUM" default="20" />
    <arg name="NAME" default="hummingbird" />
    <arg name="posfile" default="$(find coverage_unimore_nyu)/pos.txt"/>
    

